model Candidacy {
  id         Int       @id @default(autoincrement())
  announceId Int?
  userId     Int?
  announce   Announce? @relation(fields: [announceId], references: [id])
  user       User?     @relation(fields: [userId], references: [id])
}

model Announce {
  id          Int         @id @default(autoincrement())
  title       String
  candidacies Candidacy[]
}
